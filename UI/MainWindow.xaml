<Window x:Class="UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="55"/>
            <RowDefinition Height="55"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBox Name="fileLocation" Text="" FontSize="26" VerticalContentAlignment="Center" Grid.Row="0" />
        <Button Name="readFile" Style="{StaticResource TestButton}" Grid.Row="1" FontSize="26" Content="Read ExcelFile" HorizontalAlignment="Left" Width="267" Click="readFile_Click" />
        <Button Name="generateWindow" Grid.Row="1" FontSize="26" Content="Generate Window" HorizontalAlignment="Right" Width="315" Click="generateWindow_Click" />
        <TextBox Name="parsedExcelContentViewer" FontSize="26" Grid.Row="2" IsReadOnly="True"  />
    </Grid>
</Window>
